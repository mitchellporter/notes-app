{
  "extends": "airbnb",
  "rules": {
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "import/newline-after-import": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "no-unused-expressions": 0,
    "prefer-rest-params": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 2
  }
}
